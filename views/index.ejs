<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Guia Perguntas</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <img src="/img/node.png" width="100px">
    
    <h1>Guia Perguntas!</h1>
    <hr>
    <h3>Lista de Perguntas</h3>

    <p>Nome: <%= nome %></p>
    <p>Linguagem: <%= lang %></p>
    <p>Empresa: <%= empresa %></p>
    <p>Inscritos: <%= inscritos %></p>

    <% if(msg == true){ %>
        <h3>Isso Ã© uma mensagem de erro!</h3>
    <%} else {%>
        <h3>Nenhum Erro</h3>
    <% } %>

    <% produtos.forEach(function(produto) { %>
        <hr>
        <h3><%= produto.nome %></h3>
        <h4><%= produto.preco %></h4>
    <% }) %>
</body>
</html>